<import src="../../../template/bill-temp.wxml" />

<view class='page-f8f'>
  <view class='flex-wrap flex-direction-col'>
    <view class='flex-item bill-info'>
      <image class='bill-info-img' src='data:image/png;base64,'></image>
    </view>
    <view class='flex-item bill-go-up'>
      <view class='bill-info-bg'>
        <view class='flex-wrap flex-direction-col bill-item-no' style='height: {{bill_top}}px;'>
           <view class='flex-none'>
            <text class='my-font-30 font-color-333' style='letter-spacing: 0.03px;'>交易时间：{{billInfo.transTime}}</text>
          </view>
          <view class='flex-none item-col-top'>
            <text class='my-font-30 font-color-999' style='letter-spacing: 0.03px;'>
            单号：{{billInfo.orderNo}}
            </text>
          </view>
          <view class='flex-none' style='padding-top:50rpx;'>
            <text class='my-font-30 font-color-333' style='letter-spacing: 0.03px;'>单品明细</text>
          </view>
          <view class='flex-none' style='padding-top:27rpx;'>
            <template is="bill-sp-title" />
          </view>
          <view class='flex-none crossed' style='padding-top:11rpx;'>
          </view>
          <scroll-view scroll-y='true' style="height: {{scrollHeight}}px;" class='flex-none'>
            <block wx:if="{{billInfo.goodsList.length>0}}">
              <block wx:for="{{billInfo.goodsList}}" wx:key='*spList'>
                <view class='flex-item' style="padding-bottom:20rpx;padding-top:{{index == 0?30:0}}rpx">
                  <template is="bill-sp-tp" data="{{...item }}" />
                </view>
              </block>
            </block>
          </scroll-view>
          <view class='flex-none crossed'>
          </view>
          <view class='flex-none bill-cach-range'>
            <text class='my-font-30-m font-color-666' style='letter-spacing: 0.03px;'>总金额：{{billInfo.money}}</text>
          </view>
          <view class='flex-none flex-center'>
            <view class='bill-btn' bindtap='viewTicket' data-img='{{billInfo.picUrl}}'>
              <text class='my-font-32 font-color-333 top flex-center'>电子小票</text>
            </view>
          </view>
          <view class='flex-none' style='padding-top:20rpx;'>
            <view class='flex-wrap flex-direction-row pull-right' style='width:150rpx;' bindtap='reportError'>
              <view class='flex-none flex-left'>
                <image src='data:image/png;base64,' class='img-pic-30'></image>
              </view>
              <view class='flex-item flex-right'>
                <text class='my-font-24' style="color:{{billInfo.hasErrorFlag?'#FF0000':'#333'}}">我要报错</text>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>
<zan-toptips id="zan-toptips" content="{{ content }}" is-show="{{ $zanui.toptips.show }}" />