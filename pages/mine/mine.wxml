<!--index.wxml-->
<view class="page">
  <view class='top-bg'></view>
  <view class='my-panel'>
    <view class='flex-wrap flex-direction-row my-flex-panel'>
      <view class='flex-item flex-center'>
        <image src='{{userInfo.avatarUrl}}' class='my-img'></image>
      </view>
      <view class='my-flex-center' style='flex-grow:3;'>
        <text class='my-user'>{{userInfo.name}}</text>
      </view>
    </view>
  </view>
  <view class='my-list'>
    <view class='my-list-item'>
      <view class='flex-wrap flex-direction-col my-flex-panel top'>
        <view class='flex-item' style='height:100rpx;' bindtap='goMyCard'>
          <view class='flex-wrap flex-direction-row top'>
            <view class='flex-item flex-center'>
              <image class='mine-img' src='data:image/png;base64,'></image>
            </view>
            <view class='my-flex-center' style='flex-grow:6;'>
              <text class='my-list-item-name'>卡券</text>
            </view>
            <view class='my-flex-sign'>
              <image class='my-list-sign' src='data:image/png;base64,'></image>
            </view>
          </view>
        </view>
        <view class='flex-item' style='padding:0 30rpx;'>
          <view class='crossed'></view>
        </view>
        <view class='flex-item' style='height:100rpx;' bindtap='goCollect'>
          <view class='flex-wrap flex-direction-row top'>
            <view class='flex-item flex-center'>
              <image class='mine-img-40' src='data:image/png;base64,'></image>
            </view>
            <view class='my-flex-center' style='flex-grow:6;'>
              <text class='my-list-item-name'>关注</text>
            </view>
            <view class='my-flex-sign'>
              <image class='my-list-sign' src='data:image/png;base64,'></image>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>