<wxs src="module.wxs" module="tools" />

<template name="card-list-tp">
  <block wx:if="{{fetchFlag}}">
    <view class="my-card-color">
      <navigator url="/pages/card-detail/card-detail?id={{id}}&&&personId={{personCouponId}}" hover-class="none">
        <view class="flex-wrap flex-direction-row">
          <view class="flex-item flex-center">
            <image src="{{shopPhotoUrl}}" class="img-pic"></image>
          </view>
          <view class="flex-item flex-wrap flex-direction-col item-grow5" style="padding-left:25rpx">
            <view class="flex-item">
              <text class="my-font-36-m font-color-333">{{name}}</text>
            </view>
            <view class="flex-item">
              <text class="my-font-24 font-color-333 text">{{beginTime}} 至 {{endTime}}</text>
            </view>
            <view class="flex-item">
              <view class='flex-wrap flex-direction-row'>
                <view class='flex-label'>
                  <text class="my-font-24 font-color-666 text">{{shopName}}</text>
                </view>
                <view class='flex-label'>
                  <image class='img-pic-tag' src='/image/VIP@2x.png' wx:if='{{shopAuthFlag}}' />
                </view>
              </view>
            </view>
          </view>
          <view class='flex-item'></view>
          <view class="flex-item go-used my-bg-e4c">
            <view class='flex-wrap flex-direction-row top'>
              <view class="font-wrap flex-center">
                <text class='my-font-32 font-color-fff flex-center'>去使用</text>
              </view>
            </view>
          </view>
        </view>
      </navigator>
    </view>
  </block>
  <block wx:else>
    <view class="my-card-color">
      <view class="flex-wrap flex-direction-row">
        <view class="flex-item item-grow8">
          <navigator url="/pages/card-detail/card-detail?id={{id}}&&personId={{personCouponId}}" hover-class="none">
            <view class="flex-wrap flex-direction-row">
              <view class="flex-item flex-center">
                <image src="{{shopPhotoUrl}}" class="img-pic"></image>
              </view>
              <view class="flex-item flex-wrap flex-direction-col item-grow5" style="padding-left:25rpx">
                <view class="flex-item">
                  <text class="my-font-36-m font-color-333">{{name}}</text>
                </view>
                <view class="flex-item">
                  <text class="my-font-24 font-color-333 text">{{beginTime}} 至 {{endTime}}</text>
                </view>
                <view class="flex-item">
                  <view class='flex-wrap flex-direction-row'>
                    <view class='flex-label'>
                      <text class="my-font-24 font-color-666 text">{{shopName}}</text>
                    </view>
                    <view class='flex-label'>
                      <image class='img-pic-tag' src='/image/VIP@2x.png' wx:if='{{shopAuthFlag}}' />
                    </view>
                  </view>
                </view>
              </view>
              <view class='flex-item'></view>
            </view>
          </navigator>
        </view>
        <view class="flex-item go-get my-bg-fff">
          <view class='flex-wrap flex-direction-row top'>
            <view class="font-wrap flex-center" bindtap="onCard" data-id="{{id}}">
              <text class='my-font-32 my-color-e4c flex-center'>领取</text>
            </view>
          </view>
        </view>
      </view>
    </view>
  </block>
</template>

<template name="card-mydetail-tp">
  <view class="my-card-tp">
    <view class="card-bg">
      <view class="flex-wrap flex-direction-col card-list-range">
        <view class="flex-item flex-center">
          <image src="{{shopPhotoUrl?shopPhotoUrl:'/image/default-img.png'}}" class="card-img"></image>
        </view>
        <view class="flex-item flex-center">
          <view class="flex-wrap flex-direction-row">
            <view class="flex-item">
              <text class="my-font-32 font-color-333 my-space-0">{{shopName}}</text>
              <image src="/image/VIP@2x.png" wx:if="{{shopAuthFlag}}" class="img-pic-tag"></image>
            </view>
          </view>
        </view>
        <view class="flex-item flex-center" style="padding-top:30rpx">
          <text class="my-font-32 font-color-333 my-space-1">{{name}}</text>
        </view>
        <view class="flex-item flex-center">
          <canvas class="card-qrcode-img" canvas-id="mycanvas"></canvas>
        </view>
        <view class="flex-item flex-center">
          <text class="my-font-28 font-color-666 my-space-1">{{couponCode}}</text>
        </view>
        <view class="flex-item" style="padding-top:30rpx">
          <text class="my-font-28 font-color-999 my-space-1">有效期：{{beginTime}} 至 {{endTime}}</text>
        </view>
        <view class="flex-item" style="padding-top:20rpx">
          <text class="my-font-28 font-color-999 my-space-1">使用条件：{{useCondition}}</text>
        </view>
        <view class="flex-item" style="padding-top:20rpx">
          <text class="my-font-28 font-color-999 my-space-1">使用说明：</text>
        </view>
        <view class="flex-item" style="padding-top:10rpx;padding-bottom:50rpx">
          <text class="my-font-28 font-color-999 my-space-1">{{description}}</text>
        </view>
      </view>
    </view>
  </view>
</template>

