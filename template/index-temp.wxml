<import src="result-temp.wxml" />
<wxs src="module.wxs" module="tools" />

<template name="index-list-tp">
  <navigator url='../product/product?spId={{id}}&&shopId={{shopId}}' hover-class="none">
    <view class='flex-wrap flex-direction-col'>
      <view class='flex-item'>
        <view class='flex-wrap flex-direction-row'>
          <view class='flex-item'>
            <image class='img-pic img-border' src='{{picUrlList}}' />
          </view>
          <view class='flex-item' style='flex-grow:3;'>
            <view class='flex-wrap flex-direction-col'>
              <view class='flex-item item-top'>
                <text class='my-font-32 font-color-333'>{{name}}</text>
              </view>
              <view class='flex-item'>
                <view class='flex-wrap flex-direction-row'>
                  <view class='flex-item item-grow5'>
                    <text class='my-font-24 font-color-666'>{{shopName}}</text>
                    <image class='img-pic-tag' src='/image/VIP@2x.png' wx:if='{{shopAuthFlag}}' />
                  </view>
                  <view class='flex-item flex-right'>
                    <text class='my-font-24 font-color-666'>{{distance}}km</text>
                  </view>
                </view>
              </view>
              <view class='flex-item'>
                <view class='flex-wrap flex-direction-row'>
                  <block wx:for='{{recommendList}}' wx:key='*list'>
                    <view class='flex-none' style='padding-right:12rpx;'>
                      <template is="recommend-tp" data="{{...item }}" />
                    </view>
                  </block>
                  <view class='flex-item flex-right '>
                    <text class='my-font-38-m font-color-ba8'>￥{{price}}</text>
                  </view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
      <view class='flex-item scroll-border-color-d8d item-range-top'></view>
    </view>
  </navigator>
</template>

<template name='recommend-tp'>
  <block wx:if="{{type == 'entry_price_down'}}">
    <image src='/image/down_icon@2x.png' class='recommend-img'></image>
  </block>
  <block wx:if="{{type == 'entry_new'}}">
    <image src='/image/new_icon@2x.png' class='recommend-img'></image>
  </block>
  <block wx:if="{{type == 'entry_popular_follow'}}">
    <image src='/image/hot_icon@2x.png' class='recommend-img'></image>
  </block>
  <text class='my-font-24 font-color-ba8'>{{label}}</text>
</template>

<template name="collect-list-tp">
  <view class='flex-item item-range' bindlongpress="deleteImage" data-id='{{id}}' data-shopid='{{shopId}}' style="display:{{tools.getFlag(del_id,id)?'none':''}};">
    <navigator url='../product/product?spId={{id}}&&shopId={{shopId}}' hover-class="none">
      <view class='flex-wrap flex-direction-col'>
        <view class='flex-item'>
          <view class='flex-wrap flex-direction-row'>
            <view class='flex-item'>
              <image class='img-pic img-border' src='{{picUrlList}}' />
            </view>
            <view class='flex-item' style='flex-grow:3;'>
              <view class='flex-wrap flex-direction-col'>
                <view class='flex-item item-top'>
                  <text class='my-font-32 font-color-333'>{{name}}</text>
                </view>
                <view class='flex-item'>
                  <view class='flex-wrap flex-direction-row'>
                    <view class='flex-item item-grow5'>
                      <text class='my-font-24 font-color-666'>{{shopName}}</text>
                      <image class='img-pic-tag' src='/image/VIP@2x.png' wx:if='{{shopAuthFlag}}' />
                    </view>
                    <view class='flex-item flex-right'>
                      <text class='my-font-24 font-color-666'>{{distance}}km</text>
                    </view>
                  </view>
                </view>
                <view class='flex-item'>
                  <view class='flex-wrap flex-direction-row'>
                    <block wx:for='{{recommendList}}' wx:key='*list'>
                      <view class='flex-none' style='padding-right:12rpx;'>
                        <template is="recommend-tp" data="{{...item }}" />
                      </view>
                    </block>
                    <view class='flex-item flex-right '>
                      <text class='my-font-38-m font-color-ba8'>￥{{price}}</text>
                    </view>
                  </view>
                </view>
              </view>
            </view>
          </view>
        </view>
        <view class='flex-item scroll-border-color-d8d item-range-top'></view>
      </view>
    </navigator>
  </view>

</template>

<template name='index-top-tp'>
  <view class='flex-wrap flex-direction-row top '>
    <view class='flex-item flex-center' catchtap='getMyAddress'>
      <image class='mine-img-40' src='data:image/png;base64,'></image>
    </view>
    <view class='flex-item flex-center'>
      <scroll-view scroll-x='true' class='top scroll-view' scroll-into-view="{{tabType}}" scroll-left='{{scrollLeft}}' bindscroll="myScroll">
        <view class='flex-wrap flex-direction-row top'>
          <block wx:for='{{tagList}}' wx:key='*tag'>
            <view class="flex-item flex-center item-range-right {{activeIndex == index ? 'scroll-border-color-d9a' : ''}}" id="{{item.id}}" bindtap="tabClick" data-index='{{index}}'>
              <text class="my-font-32 {{activeIndex == index ? 'font-color-ba8' : 'font-color-666'}}">{{item.name}}</text>
            </view>
          </block>
        </view>
      </scroll-view>
    </view>
    <view class='flex-item flex-center' style='flex-grow:0.3;'>
      <view class='border-right' style='height:50rpx;'></view>
    </view>
    <view class='flex-item flex-left' bindtap='getResultPage' style='flex-grow:0.8;'>
      <image src='data:image/png;base64,'
        class='index-img'></image>
    </view>
  </view>
</template>

<template name='index-search-tp'>
  <view class='flex-wrap flex-direction-row top' style='padding:0 30rpx;'>
    <view class='flex-item flex-center' catchtap='getFilter' style='flex-grow:0.5;'>
      <view class='flex-wrap flex-direction-row top'>
        <view class='flex-item flex-center'>
          <text class="my-font-28 {{filter_ck?'font-color-ba8':'font-color-666'}}">{{filter_word}}</text>
        </view>
        <view class='flex-item flex-left'>
          <image class='img-pic-10' src="/image/down_icon_normal@2x.png"></image>
        </view>
      </view>
    </view>
    <view class='flex-item flex-center' style='flex-grow:0.5;' bindtap='getShop'>
      <view class='flex-wrap flex-direction-row top'>
        <view class='flex-item flex-center'>
          <text class="my-font-28 {{shop_ck?'font-color-ba8':'font-color-333'}}">{{my_shop_ck}}</text>
        </view>
        <view class='flex-item flex-left'>
          <image class='img-pic-10' src="/image/down_icon_normal@2x.png"></image>
        </view>
      </view>
    </view>
    <view class='flex-item flex-right' bindtap='listTap'>
      <view class='erect-border'></view>
    </view>
    <view class='flex-item flex-center' style='flex-grow:0.5;' bindtap='getSort'>
      <view class='flex-wrap flex-direction-row top'>
        <view class='flex-item flex-center'>
          <text class="my-font-28 {{sort_ck?'font-color-ba8':'font-color-666'}}">筛选</text>
        </view>
        <view class='flex-item flex-left'>
          <image class='img-pic-30' src="/image/{{sort_ck?'icon_screen_selected@2x.png':'icon_screen@2x.png'}}"></image>
        </view>
      </view>
    </view>
  </view>
</template>

<template name='index-reflash-tp'>
  <view class='card-ps' bindtap='reflashDowm'>
    <image class='imge-reflash' animation="{{animationData}}" src='data:image/png;base64,'></image>
  </view>
</template>

<template name='index-filter-tp'>
  <view class="flex-item filter-range" bindtap='mySelect' data-my="{{id}}" data-name='{{name}}'>
    <text class="my-font-28 {{sort_select_id==id?'font-color-ba8':'font-color-666'}}">{{name}}</text>
  </view>
  <view class='flex-item filter-range'>
    <view class='crossed'></view>
  </view>
</template>

<template name='index-shop-tp'>
  <view class="flex-item filter-range" bindtap='myShop' data-my="{{id}}" data-name='{{name}}'>
    <text class="my-font-28 {{my_shop_ck==name?'font-color-ba8':'font-color-666'}}">{{name}}</text>
  </view>
  <view class='flex-item filter-range'>
    <view class='crossed'></view>
  </view>
</template>

<template name='index-sort-tp'>
  <view class='flex-item filter-range'>
    <view class='flex-wrap flex-direction-row item-wrap'>
      <view class='flex-none flex-center scroll-view-item_H'>
        <text class='my-font-28 font-color-666'>单品标签</text>
      </view>
      <block wx:for='{{sort_list}}' wx:key='*filter2'>
        <block wx:if='{{filterLabels.length>0}}'>
          <template is="filter-inster-tp" data="{{...item,filterLabels:filterLabels}}" />
        </block>
        <block wx:else>
          <view class='flex-label-item' bindtap='getTabSel' data-id='{{item.id}}'>
            <text class="hot-item-name my-font-24 font-color-666">{{item.name}}</text>
          </view>
        </block>
      </block>
    </view>
  </view>
  <view class='flex-item'>
    <view class='crossed'></view>
  </view>
  <view class='flex-item' style='width:100%;height:80rpx;'>
    <view class='flex-wrap flex-direction-row top'>
      <view class='flex-item flex-center border-right' catchtap='getReset'>
        <text class='my-font-30 font-color-666'>重置</text>
      </view>
      <view class='flex-item flex-center' catchtap='getEnuser'>
        <text class='my-font-30 font-color-666'>确认</text>
      </view>
    </view>
  </view>
</template>

<template name='index-empty-tp'>
  <view class='flex-wrap flex-direction-col top'>
    <view class='flex-item flex-center'>
      <image src='data:image/png;base64,'
        class='img-pic-295'></image>
    </view>
    <view class='flex-item flex-left'>
      <text class='text-center' style='width: 100%;'>请选择其他标签吧~</text>
    </view>
  </view>
</template>